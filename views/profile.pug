h1 #{title}

h2 Username: #{username}

h3 Total PP: #{pptotal}

ul(type="None")
    each val, index in entries
        li
            b= (index+1)
                =". " + val[1]
        li= "-> Raw PP: " 
            b= val[2]
                small= " (Net pp: " + (val[2]*(Math.pow(0.95,index))).toFixed(2) + ")"
        <li>--------------------------------------------------------------------------</li>